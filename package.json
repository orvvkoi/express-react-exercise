{
    "name": "express-react-practice",
    "version": "1.0.0",
    "main": "server/index.js",
    "scripts": {
        "build-server": "babel server -d build",
        "build-client": "cross-env NODE_ENV=production webpack --config config/webpack.config.prod.js",
        "start": "npm run build-client && npm run build-server && node build/index.js",
        "client": "cross-env NODE_ENV=development webpack serve --config config/webpack.config.dev.js",
        "server": "nodemon server/index.js --exec babel-node",
        "dev": "concurrently \"npm run server\" \"npm run client\""
    },
    "dependencies": {
        "axios": "^0.21.1",
        "concurrently": "^6.0.0",
        "cookie-parser": "~1.4.4",
        "core-js": "^3.9.1",
        "cors": "^2.8.5",
        "debug": "~2.6.9",
        "dotenv": "^8.2.0",
        "express": "^4.16.4",
        "express-session": "^1.17.1",
        "morgan": "~1.9.1",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-router-dom": "^5.2.0",
        "regenerator-runtime": "^0.13.7",
        "request": "^2.88.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.12.10",
        "@babel/core": "^7.13.10",
        "@babel/eslint-parser": "^7.13.8",
        "@babel/node": "^7.13.0",
        "@babel/preset-env": "^7.12.11",
        "@babel/preset-react": "^7.12.13",
        "babel-loader": "^8.2.2",
        "babel-plugin-root-import": "^6.6.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "clean-webpack-plugin": "^3.0.0",
        "cross-env": "^7.0.3",
        "css-loader": "^5.1.1",
        "css-minimizer-webpack-plugin": "^1.2.0",
        "eslint": "^7.17.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.22.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "html-loader": "^2.1.2",
        "html-webpack-plugin": "^5.2.0",
        "mini-css-extract-plugin": "^1.3.9",
        "mocha": "^8.2.1",
        "nodemon": "^2.0.7",
        "prettier": "^2.2.1",
        "react-hot-loader": "^4.13.0",
        "style-loader": "^2.0.0",
        "webpack": "^5.24.4",
        "webpack-cli": "^4.5.0",
        "webpack-dev-server": "^3.11.2",
        "webpack-merge": "^5.7.3"
    }
}
